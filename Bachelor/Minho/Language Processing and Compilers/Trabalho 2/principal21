PUSHI 0
START
PUSHS "Procurar um valor na matriz e contar quantas vezes ele aparece\n"
WRITES
PUSHS "Digite o valor a procurar: \n"
WRITES
READ
ATOI
STOREG 0
WHILE1:
PUSHG 2
PUSHI 3
INF
JZ ENDWHILE1
WHILE0:
PUSHG 3
PUSHI 3
INF
JZ ENDWHILE0
PUSHG 0
PUSHGP
PUSHI 4
PADD
PUSHG 2
PUSHI 3
MUL
PUSHG 3
ADD
LOADN
EQUAL
JZ IF0
PUSHI 1
STOREG 1
PUSHG 13
PUSHI 1
ADD
STOREG 13
PUSHG 3
PUSHI 1
ADD
STOREG 3
JUMP IFEND0
IF0:
PUSHG 3
PUSHI 1
ADD
STOREG 3
IFEND0:
JUMP WHILE0
ENDWHILE0:
PUSHG 2
PUSHI 1
ADD
STOREG 2
PUSHI 0
STOREG 3
JUMP WHILE1
ENDWHILE1:
PUSHG 1
PUSHI 1
EQUAL
JZ IF1
PUSHS "Valor encontrado: "
WRITES
PUSHG 0
WRITEI
PUSHS "\n numero de vezes encontrado: "
WRITES
PUSHG 13
WRITEI
JUMP IFEND1
IF1:
PUSHS "Valor nao encontrado\n"
WRITES
IFEND1:
STOP